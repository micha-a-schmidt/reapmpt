 RowBox[{
  RowBox[{
   RowBox[{"CompareToExperiment", "[", "t_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"lNotComp", ",", "lComp"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"lComp", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"lNotComp", "=", 
       RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Sin", "[", 
            RowBox[{
             RowBox[{"\[Theta]12", "[", "t", "]"}], " ", "Degree"}], "]"}], 
           "^", "2"}], "\[GreaterEqual]", "0.23"}], "&&", 
         RowBox[{
          RowBox[{
           RowBox[{"Sin", "[", 
            RowBox[{
             RowBox[{"\[Theta]12", "[", "t", "]"}], " ", "Degree"}], "]"}], 
           "^", "2"}], "\[LessEqual]", "0.38"}]}], ",", 
        RowBox[{"lComp", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"lComp", ",", "\"\<\[Theta]12\>\""}], "]"}]}], ",", 
        RowBox[{"lNotComp", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"lNotComp", ",", "\"\<\[Theta]12\>\""}], "]"}]}]}], "]"}], 
      ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
